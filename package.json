{
  "name": "@alumni-ventures-group/front-end-components",
  "version": "1.0.1",
  "description": "front end components for react",
  "main": "rollup.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "watch": "rollup -cw",
    "build": "rm -rf lib && rollup -c",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alumni-ventures-group/front-end-components.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/alumni-ventures-group/front-end-components/issues"
  },
  "homepage": "https://github.com/alumni-ventures-group/front-end-components#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "autoprefixer": "^10.2.6",
    "node-sass": "^4.14.1",
    "npm": "^7.11.2",
    "postcss": "^8.3.0",
    "postcss-url": "^10.1.3",
    "rollup": "^2.50.5",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "semantic-release": "^17.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-styleguidist": "^11.1.6",
    "sass-loader": "^12.0.0",
    "webpack": "^5.38.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@alumni-ventures-group"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
